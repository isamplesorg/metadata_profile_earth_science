  skos:broader sesrs:Fragmental_Igneous_Material ;
  skos:broader sesrs:Volcanic_Material ;
  skos:inScheme sesrs:rocksedimentvocabulary ;
  skos:prefLabel "pyroclastic material"@en ;
.
